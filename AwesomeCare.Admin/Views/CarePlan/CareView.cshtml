                <h4>Nutrition</h4>
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12">
                        @if (Model.NutritionId > 0)
                        {
                            <div class="row">
                                <div class="form-group col-sm-4 col-md-4 col-xs-12 col-lg-4">
                                    <label class="control-label">Food Storage</label>
                                    <baserecord asp-key="Food_Storage" asp-for="Nutrition_FoodStorage" asp-items="null" />
                                    <span asp-validation-for="Nutrition_FoodStorage" class="text danger"></span>
                                </div>
                                <div class="form-group col-sm-4 col-md-4 col-xs-12 col-lg-4">
