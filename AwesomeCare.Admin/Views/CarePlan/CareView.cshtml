                <h4>Home Risk Assessment</h4>
                <div class="row">
                    <div class="col-lg-12 col-xs-12 col-sm-12 col-md-12">
                        <div class="card">
                            <div class="header">
                                <h2>Home Risk Assessment Headings</h2>

                            </div>
                            <div class="body">
                                @if (Model.GetHomeRiskAssessments.Count > 0)
                                {
                                    <table class="table table-bordered table-hover" id="homeRisk">
                                        <thead>
                                            <tr>
                                                <th>Heading</th>
                                                <th>Tasks</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @foreach (var item in Model.HeadingList)
                                            {
                                                <tr>
                                                    <td>
                                                        @item.Text
                                                    </td>
                                                    <td>


                                                        @foreach (var task in Model.GetHomeRiskAssessments.Where(s => s.HomeRiskAssessmentId.ToString() == item.Value).Select(s => s.GetHomeRiskAssessmentTask).FirstOrDefault())
                                                        {
                                                            <table>
                                                                <thead>
                                                                    <tr>
                                                                        <th>Title</th>
                                                                        <th>Answer</th>
                                                                        <th>Comment</th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    <tr>
                                                                        <td style="white-space: normal; text-align: justify;">@task.TitleName</td>
                                                                        <td>@task.AnswerName</td>
                                                                        <td style="white-space: normal; text-align: justify;">@task.Comment</td>
                                                                    </tr>
                                                                </tbody>
                                                            </table>
                                                        }


                                                    </td>
                                                </tr>


                                            }
                                        </tbody>
                                    </table>
                                }
                            </div>
                        </div>
                    </div>
                </div>
