        <script>
            $('#btnHide').on('click', function () {

                if ($('#sidebar').css('display') == 'none') {
                    $(this).children().first().removeClass('fa fa-arrow-right');
                    $(this).children().first().addClass('fa fa-arrow-left');
                    $('#sidebar').css('display', 'block');
                    $('.navbar-right').css('display', 'block');
                    $('#header').css('display', 'block');
                    $('#main-content').css('float', 'right');
                    $('#main-content').css('margin-left', '0px');
                }
                else {
                    $(this).children().first().removeClass('fa fa-arrow-left');
                    $(this).children().first().addClass('fa fa-arrow-right');
                    $('#sidebar').css('display', 'none');
                    $('.navbar-right').css('display', 'none');
                    $('#header').css('display', 'none');
                    $('#main-content').css('float', 'none');
                    $('#main-content').css('margin-left', '-100px');
                }
            });
            $(document).ready(function () {

                $('.dropdown').css('padding', '0px');
                // Multiselect
                //$('.multiselect').multiselect({
                //    maxHeight: 300
                //});
                $('.multiselectfilter').multiselect({
                    enableFiltering: true,
                    enableCaseInsensitiveFiltering: true,
                    maxHeight: 200
                });

                var date = new Date();
                date.setDate(date.getDate());
                $('.datepickerfromtoday').datepicker({
                    startDate: date,
                    todayBtn: true
                })

                $('.datepickerbeforeOrtoday').datepicker({
                    endDate: date,
                    todayBtn: true
                })
                $(".chosen-select").chosen({ no_results_text: "Oops, nothing found!" });

                $(".datatable").DataTable({
                    destroy: true,
                    searching: true
                });

                $(".datatables").DataTable({
                    searching: true,
                    pageLength: 5,
                    lengthMenu: [[5, 10, 20, -1], [5, 10, 20, 'Todos']]
                });

                $('.datatablesExportExcel').DataTable({
                    dom: 'Blfrtip',
                    buttons: [
                        'excel'
                    ],
                    aLengthMenu: [
                        [10, 20, 50, 100, -1],
                        [10, 20, 50, 100, "All"]
                    ]
                });
            })</script>

