
@using AwesomeCare.DataTransferObject.DTOs.StaffWorkTeam;
@model PostStaffWorkTeam
        @{
        ViewData["Title"] = "Create";
        }
               
        <div class="row clearfix">
            <div class="col-lg-12 col-md-12">
                <div class="card planned_task">
                    <div class="header">
                        <h2>Create Staff Work Team</h2>
                        
                    </div>
                    <div class="body">
                       
                        <form asp-route="Create" method="post">
                            <div class="row">

                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label asp-for="WorkTeam"></label>
                                    <input type="text" asp-for="WorkTeam" class="form-control">
                                    <span asp-validation-for="WorkTeam" class="text-danger"></span>
                                </div>

                            </div>



                            <button type="submit" class="btn btn-primary">Submit</button>
                            <a asp-action="Index" asp-controller="StaffWorkTeam" class="btn btn-primary pull-right">Cancel</a>
                        </form>
                    </div>
                </div>
            </div>
        </div>
 
