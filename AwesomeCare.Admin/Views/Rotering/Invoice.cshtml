@using AwesomeCare.DataTransferObject.DTOs.Rotering;
@model List<LiveTracker>
@{
    ViewData["Title"] = "Client Invoice";
}
<form asp-action="Invoice" method="post">
    <div class="row">
        <div class="col-sm-6 col-xs-12 col-lg-6 col-md-6">
            <div class="row">

                <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                    <label>Select Client</label>
                    <select name="clientId" asp-items="@ViewBag.GetClients" class="selectpicker form-control" data-live-search="true"></select>
                </div>

            </div>
        </div>
        <div class="col-sm-6 col-xs-6 col-lg-6">
            <div class="row">
                <div class="form-group col-sm-6 col-xs-12 col-md-6 col-lg-6">
                    <label> Start Date</label>
                    <input type="text" id="startDate" name="startDate" class="form-control datepickerbeforeOrtoday" data-date-format="yyyy-mm-dd">
                </div>
                <div class="form-group col-sm-6 col-xs-12 col-md-6 col-lg-6">
                    <label> Stop Date</label>
                    <input type="text" id="stopDate" name="stopDate" class="form-control datepickerbeforeOrtoday" data-date-format="yyyy-mm-dd">
                </div>
            </div>
        </div>
        <button type="submit" class="btn btn-primary" id="btnInvoice">Search</button>
    </div>
</form>
<script src="~/assets2/js/jquery-3.6.0.min.js"></script>
<script type="text/javascript">

</script>