@using AwesomeCare.Admin.ViewModels.Staff;
@model CreateStaffSupervisionAppraisal
@{
    ViewData["Title"] = "Update Staff Supervision Appraisal";
}
@section HeaderScripts{
    <link rel="stylesheet" href="../../../assets2/vendor/jquery-steps/jquery.steps.css">
    <link rel="stylesheet" href="../../../assets2/vendor/dropify/css/dropify.min.css">
}
<div class="row clearfix">
    <div class="col-lg-12 col-md-12">
        <div class="card planned_task">
            <div class="card-header">

                <h2></h2>

            </div>
            <div class="card-body">
                <form asp-route="Edit" method="post" enctype="multipart/form-data">
                    <div class="row">
                        <div class="col-xs-8 col-sm-8 col-md-8">
                            <input type="hidden" asp-for="StaffSupervisionAppraisalId" />
                            <input type="hidden" asp-for="StaffId" />
                            <input type="hidden" asp-for="Attachment" />
                            <div class="row">
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">DATE</label>
                                    <input type="text" asp-for="Date" class="form-control" data-provide="datepicker" data-date-autoclose="true" data-date-format="mm/dd/yyyy" />
                                    <span asp-validation-for="Date" class="text-danger"></span>
                                </div>
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">NEXT DUE DATE</label>
                                    <input type="text" asp-for="NextCheckDate" class="form-control" data-provide="datepicker" data-date-autoclose="true" data-date-format="mm/dd/yyyy" />
                                    <span asp-validation-for="NextCheckDate" class="text-danger"></span>
                                </div>
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">DEADLINE</label>
                                    <input type="text" asp-for="Deadline" class="form-control" data-provide="datepicker" data-date-autoclose="true" data-date-format="mm/dd/yyyy" />
                                    <span asp-validation-for="Deadline" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">WorkTeam</label>
                                    <select asp-for="WorkTeam" asp-items="@Model.OfficerToActList" class="selectpicker form-control" data-live-search="true" multiple></select>
                                    <span asp-validation-for="WorkTeam" class="text-danger"></span>
                                </div>
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">Staff Complaints </label>
                                    <baserecord asp-key="Staff_Complaints" asp-for="StaffComplaints" asp-items="null" />
                                    <span asp-validation-for="StaffComplaints" class="text-danger"></span>
                                </div>
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">Undergoing Staff Development</label>
                                    <baserecord asp-key="Staff_Development" asp-for="ProfessionalDevelopment" asp-items="null" />
                                    <span asp-validation-for="ProfessionalDevelopment" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">Staff Feedback Rating</label>
                                    <baserecord asp-key="Staff_Feedback_Rating" asp-for="StaffRating" asp-items="null" />
                                    <span asp-validation-for="StaffRating" class="text-danger"></span>
                                </div>
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">Details</label>
                                    <input type="text" asp-for="Details" class="form-control" />
                                    <span asp-validation-for="Details" class="text-danger"></span>
                                </div>
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">Officer to Take Action</label>
                                    <select asp-for="OfficerToAct" asp-items="@Model.OfficerToActList" class="selectpicker form-control" data-live-search="true" multiple></select>
                                    <span asp-validation-for="OfficerToAct" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">Five Star Rating</label>
                                    <input type="text" asp-for="FiveStarRating" class="form-control" />
                                    <span asp-validation-for="FiveStarRating" class="text-danger"></span>
                                </div>
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">Status</label>
                                    <baserecord asp-key="Staff_Communication_Status" asp-for="Status" asp-items="null" />
                                    <span asp-validation-for="Status" class="text-danger"></span>
                                </div>
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">Development on Job</label>
                                    <input type="text" asp-for="StaffDevelopment" class="form-control" />
                                    <span asp-validation-for="StaffDevelopment" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">Reference</label>
                                    <input type="text" asp-for="Reference" class="form-control" />
                                    <span asp-validation-for="Reference" class="text-danger"></span>
                                </div>
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">Staff Support Areas</label>
                                    <baserecord asp-key="Staff_Support_Areas" asp-for="StaffSupportAreas" asp-items="null" />
                                    <span asp-validation-for="StaffSupportAreas" class="text-danger"></span>
                                </div>
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">Emergency Assisting Ability</label>
                                    <input type="text" asp-for="StaffAbility" class="form-control" />
                                    <span asp-validation-for="StaffAbility" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                    <label class="control-label">URL</label>
                                    <input type="text" asp-for="URL" class="form-control" />
                                    <span asp-validation-for="URL" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                    <label class="control-label">Remarks</label>
                                    <textarea type="text" asp-for="Remarks" class="form-control"></textarea>
                                    <span asp-validation-for="Remarks" class="text-danger"></span>
                                </div>
                                <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                    <label class="control-label">No Ability To Support</label>
                                    <textarea type="text" asp-for="NoAbilityToSupport" class="form-control"></textarea>
                                    <span asp-validation-for="NoAbilityToSupport" class="text-danger"></span>
                                </div>
                                <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                    <label class="control-label">Condour And Whistle Blowing</label>
                                    <textarea type="text" asp-for="CondourAndWhistleBlowing" class="form-control"></textarea>
                                    <span asp-validation-for="CondourAndWhistleBlowing" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                    <label class="control-label">Condour And Whistle Blowing Consequences</label>
                                    <textarea type="text" asp-for="NoCondourAndWhistleBlowing" class="form-control"></textarea>
                                    <span asp-validation-for="NoCondourAndWhistleBlowing" class="text-danger"></span>
                                </div>
                                <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                    <label class="control-label">Action Required</label>
                                    <textarea type="text" asp-for="ActionRequired" class="form-control"></textarea>
                                    <span asp-validation-for="ActionRequired" class="text-danger"></span>
                                </div>
                            </div>

                        </div>
                        <div class="col-xs-4 col-sm-4 col-md-4">
                            <div class="card">
                                <div class="card-header">
                                    <h2>Attachment Image<small></small></h2>
                                </div>
                                <div class="card-body">
                                    <input type="file" asp-for="Attach" class="dropify" accept=".png, .jpg, .jpeg, .pdf, .xlsx, .docx">
                                    <span asp-validation-for="Attach" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-sm-6 col-xs-12 col-md-4 col-lg-6">
                                    <button type="submit" class="btn btn-primary pull-right">Submit</button>
                                </div>
                                <div class="form-group col-sm-6 col-xs-12 col-md-4 col-lg-6">
                                    <a asp-action="Index" asp-route-staffId="@Model.StaffId" class="btn btn-primary pull-left">Cancel</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
@section VendorScripts{
    <script src="../../../assets2/vendor/jquery-steps/jquery.steps.js"></script> <!-- JQuery Steps Plugin Js -->
    <script src="../../../assets2/vendor/dropify/js/dropify.min.js"></script>
    <script src="../../../assets2/bundles/mainscripts.bundle.js"></script>
}

@section Scripts{
    <script src="../../../assets2/js/pages/forms/form-wizard.js"></script>
    <script src="../../../assets2/js/pages/forms/dropify.js"></script>
}

