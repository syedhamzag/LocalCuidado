</div>

<div class="row">
    <div class="col-xs-12 col-sm-4 col-md-4">
        <div class="form-group">
            <label asp-for="Keyworker" class="control-label"></label>
            <select asp-for="KeyworkerId" asp-items="@Model.StaffList" class="selectpicker form-control" data-live-search="true"></select>
            <span asp-validation-for="KeyworkerId" class="text-danger"></span>
        </div>
    </div>
    <div class="col-xs-12 col-sm-4 col-md-4">
        <div class="form-group">
            <label asp-for="About" class="control-label"></label>
            <textarea class="form-control" asp-for="About"></textarea>
            <span asp-validation-for="About" class="text-danger"></span>
