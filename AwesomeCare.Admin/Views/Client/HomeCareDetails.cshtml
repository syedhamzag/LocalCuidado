                <li class="nav-item col-sm-3"><a class="nav-link" data-bs-toggle="tab" href="#careobj" id="@Model.ClientId" onclick="getcareobj(this)">Care Objectives</a></li>
                <li class="nav-item col-sm-3"><a class="nav-link" data-bs-toggle="tab" href="#performance" id="@Model.ClientId" onclick="getclientperformance(this)">Performance Indicator</a></li>
                <li class="nav-item col-sm-3"><a class="nav-link" asp-action="Index" asp-controller="Nutrition" asp-route-clientId="@Model.ClientId">Nutrition</a></li>
                <li class="nav-item col-sm-3"><a class="nav-link" data-bs-toggle="tab" href="#rotering" id="@Model.ClientId" onclick="getrotering(this)">Rotering</a></li>
                <li class="nav-item col-sm-3"><a class="nav-link" asp-action="Medications" asp-controller="Client" asp-route-clientId="@Model.ClientId">Medications</a></li>
