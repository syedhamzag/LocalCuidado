                                        <tr>
                                            <td>
                                                @item.Reference
                                            </td>
                                            <td>
                                                @if (!string.IsNullOrEmpty(item.Attachment))
                                                {
                                                    <a href="#" class="on-default showfile-btn" title="Download" data-id="@item.Attachment" style="margin-left:5px;"><i class="fa fa-file"></i></a>
                                                }

                                            </td>

                                        </tr>
                                    }

                                </tbody>
                            </table>
