                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-4 col-md-4">
                                <div class="form-group">
                                    <label asp-for="Longitude" class="control-label"></label>
                                    <input type="text" class="form-control" asp-for="Longitude" readonly>
                                </div>
                            </div>

                        </div>

                        <div class="row">
                            <div class="col-xs-12 col-sm-4 col-md-4">
                                <div class="form-group">
                                    <label asp-for="Address" class="control-label"></label>
                                    <textarea class="form-control" asp-for="Address" readonly></textarea>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-4 col-md-4">
                                <div class="form-group">
                                    <label asp-for="About" class="control-label"></label>
                                    <textarea class="form-control" asp-for="About" readonly></textarea>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-4 col-md-4">
                                <div class="form-group">
                                    <label asp-for="Hobbies" class="control-label"></label>
                                    <textarea class="form-control" asp-for="Hobbies" readonly></textarea>
                                </div>
                            </div>

                        </div>
                    </div>

                    <div class="tab-pane" id="involvingparties">
                        <table class="table table-bordered table-hover">
                            <thead>
                                <tr>

                                    <th>NAME</th>
                                    <th>ADDRESS</th>
                                    <th>EMAIL</th>
                                    <th>TELEPHONE</th>
                                    <th>RELATIONSHIP</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model.InvolvingParties)
                                {
                                    <tr>
                                        <td>
                                            @item.Name
                                        </td>
                                        <td>
                                            @item.Address
                                        </td>
                                        <td>
                                            @item.Email
                                        </td>
                                        <td>
                                            @item.Telephone
                                        </td>
                                        <td>
                                            @item.Relationship
                                        </td>
                                    </tr>
                                }

                            </tbody>
                        </table>
                    </div>
                    <div class="tab-pane" id="dutyoncall">
                        <div class="header">
                            <ul class="header-dropdown">
                                <li class="dropdown">
                                    <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"></a>
                                    <ul class="dropdown-menu dropdown-menu-right">
                                        <li><a asp-action="Index" asp-route-clientId="@Model.ClientId" asp-controller="DutyOnCall">Create</a></li>


                                    </ul>
                                </li>
                            </ul>
                        </div>
                        <div class="body">
                            <table class="table table-bordered table-hover">
                                <thead>
                                    <tr>
                                        <th>Date</th>
                                        <th>RefNo</th>
                                        <th>Subject</th>
                                        <th>Attachment</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model.GetDutyOnCall)
                                    {
                                    <tr>
                                        <td>@item.DateOfCall</td>
                                        <td>
                                            @item.RefNo
                                        </td>
                                        <td>@item.Subject</td>
                                        <td>
                                            @if (!string.IsNullOrEmpty(item.Attachment))
                                            {
                                                <a href="#" class="on-default showfile-btn" title="Download" data-id="@item.Attachment" style="margin-left:5px;"><i class="fa fa-file"></i></a>
                                            }

                                        </td>

                                    </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <div class="tab-pane" id="hospitalentry">
                        <div class="header">
                            <ul class="header-dropdown">
