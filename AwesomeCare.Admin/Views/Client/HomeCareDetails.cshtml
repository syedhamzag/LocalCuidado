                                    <div class="col-xs-12 col-sm-4 col-md-4">
                                        <div class="form-group">
                                            <label asp-for="EndDate" class="control-label"></label>
                                            <input type="text" asp-for="EndDate" class="form-control" readonly>

                                        </div>
                                    </div>

                                </div>

                                <div class="row">
                                    <div class="col-xs-12 col-sm-4 col-md-4">
