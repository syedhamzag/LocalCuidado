@using  AwesomeCare.DataTransferObject.DTOs.Client;
@model GetClient
@{
    ViewData["Title"] = "HomeCare Details";
}
<div class="card mb-0">
    <div class="card-header">
        <h2>HomeCare Client Details</h2>
        <h3>@Model.PreferredName</h3>
        <div class="dropdown profile-action">
            <a href="#" class="action-icon dropdown-toggle" data-bs-toggle="dropdown" aria-expanded="false"><i class="material-icons">more_vert</i></a>
            <div class="dropdown-menu dropdown-menu-right" style="">
                <a asp-action="EditRegistration" asp-route-clientId="@Model.ClientId" asp-controller="Client">Edit Client</a>
            </div>
        </div>
    </div>
    <div class="card-body">
        <div class="row">
            <div class="col-md-12">
                <div class="profile-view">
                    <div class="profile-img-wrap">
                        <div class="profile-img">
                            <a href="">
                                <img src="@Model.PassportFilePath" alt="">
                            </a>
                        </div>
                    </div>
                    <div class="profile-basic">
                        <div class="row">
                            <div class="col-md-5">
                                <div class="profile-info-left">
                                    <h3 class="user-name m-t-0">@string.Concat(Model.Firstname, " ", Model.Middlename, " ", Model.Surname)</h3>
                                    <h5 class="company-role m-t-0 mb-0"></h5>
                                    <small class="text-muted"></small>
                                    <div class="staff-id">Client ID : @Model.IdNumber</div>
                                    <div class="staff-msg">
                                        <div class="row">
                                            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                                <a asp-route-clientId="@Model.ClientId" asp-action="Index" asp-controller="Chat" class="btn btn-custom">Send Message</a>
                                            </div>
                                            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                                @Html.ActionLink("View Care Plan", "CareView", "CarePlan", new { clientId = Model.ClientId }, new { @class = "btn btn-custom" })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-5">
                                <ul class="personal-info">
                                    <li>
                                        <span class="title">Phone:</span>
                                        <span class="text"><a href="">@Model.Telephone</a></span>
                                    </li>
                                    <li>
                                        <span class="title">Email:</span>
                                        <span class="text"><a href="">@Model.Email</a></span>
                                    </li>
                                    <li>
                                        <span class="title">Birthday:</span>
                                        <span class="text">@Model.DateOfBirth</span>
                                    </li>
                                    <li>
                                        <span class="title">Address:</span>
                                        <span class="text">@Model.Address</span>
                                    </li>
                                    <li>
                                        <span class="title">Gender:</span>
                                        <span class="text">@Model.Gender</span>
                                    </li>
                                </ul>
                            </div>
                            <div class="col-md-2">
                                <div class="profile-img-wrap">
                                    <div class="profile-img">
                                        <a href="">
                                            <img alt="QR Code" src="@String.Format("data:image/png;base64,{0}", Convert.ToBase64String(Model.QRCode))">
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    
                </div>
            </div>
        </div>
    </div>
</div>
<div class="card tab-box">
    <div class="row user-tabs">
        <div class="col-lg-12 col-md-12 col-sm-12 line-tabs">
            <ul class="nav nav-tabs nav-tabs-bottom">
                <li class="nav-item col-sm-3"><a class="nav-link active" data-bs-toggle="tab" href="#personalInfo">Personal Info</a></li>
