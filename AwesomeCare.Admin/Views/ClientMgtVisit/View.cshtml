@using AwesomeCare.Admin.ViewModels.ClientMgtVisit;
@model CreateClientMgtVisit
@{
    ViewData["Title"] = "View Client Mgt Visit";
}
@section HeaderScripts{
    <link rel="stylesheet" href="../../../assets/vendor/jquery-steps/jquery.steps.css">
    <link rel="stylesheet" href="../../../assets/vendor/dropify/css/dropify.min.css">
}
<div class="row clearfix">
    <div class="col-lg-12 col-md-12">
        <div class="card planned_task">
            <div class="header">

                <h2></h2>

            </div>
            <div class="body">
                <div class="row">
                    <div class="col-xs-8 col-sm-8 col-md-8">
                        <input type="hidden" asp-for="VisitId" />
                        <input type="hidden" asp-for="ClientId" />
                        <div class="row">

                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">DATE</label>
                                <input type="text" asp-for="Date" class="form-control" readonly />
                            </div>
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">Next Check Date</label>
                                <input type="text" asp-for="NextCheckDate" class="form-control" readonly />
                            </div>
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">Deadline</label>
                                <input type="text" asp-for="Deadline" class="form-control" readonly />
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">Rate Service Receiving</label>
                                <baserecord asp-key="Rate_Service_Recieving" asp-for="RateServiceRecieving" asp-items="null" />
                            </div>
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">Rate Service Manager</label>
                                <baserecord asp-key="Rate_Service_Manager" asp-for="RateManagers" asp-items="null" />
                            </div>
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">Staff Best Support</label>
                                <select asp-for="StaffBestSupport" asp-items="@Model.StaffList" class="selectpicker" data-live-search="true" multiple></select>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">How To Complain</label>
                                <baserecord asp-key="How_To_Complain" asp-for="HowToComplain" asp-items="null" />
                            </div>
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">Service Recommended</label>
                                <baserecord asp-key="Service_Recommended" asp-for="ServiceRecommended" asp-items="null" />
                            </div>
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">Officer to Take Action</label>
                                <select asp-for="OfficerToAct" asp-items="@Model.OfficerToActList" class="selectpicker" data-live-search="true" multiple></select>
                            </div>
                        </div>

                        <div class="row">
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">Reference</label>
                                <input type="text" asp-for="Reference" class="form-control" readonly />
                            </div>
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">Rot Cause</label>
                                <input type="text" asp-for="RotCause" class="form-control" readonly />
                            </div>
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">Log URL</label>
                                <input type="text" asp-for="URL" class="form-control" readonly />
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">Remark/Closing Comment</label>
                                <input type="text" asp-for="Remarks" class="form-control" readonly />
                            </div>
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">Status</label>
                                <baserecord asp-key="Staff_Communication_Status" asp-for="Status" asp-items="null" />
                            </div>
                        </div>


                        <div class="row">
                            <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                <label class="control-label">Improvement Do You Expect</label>
                                <textarea type="text" asp-for="ImprovementExpect" class="form-control" readonly></textarea>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                <label class="control-label">Action Required</label>
                                <textarea type="text" asp-for="ActionRequired" class="form-control" readonly></textarea>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                <label class="control-label">Actions Taken By MPCC</label>
                                <textarea type="text" asp-for="ActionsTakenByMPCC" class="form-control" readonly></textarea>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                <label class="control-label">Observations</label>
                                <textarea type="text" asp-for="Observation" class="form-control" readonly></textarea>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                <label class="control-label">Lesson Learnt and Shared</label>
                                <textarea type="text" asp-for="LessonLearntAndShared" class="form-control" readonly></textarea>
                            </div>
                        </div>


                    </div>
                    <div class="col-xs-4 col-sm-4 col-md-4">
                        <div class="card">
                            <div class="header">
                                <h2>Evidence of Action Taken<small></small></h2>
                            </div>
                            <div class="body">
                                <img src="@Model.Evidence" alt="image" width="300" height="240" />
                            </div>
                        </div>
                        <div class="card">
                            <div class="header">
                                <h2>Attachment Image<small></small></h2>
                            </div>
                            <div class="body">
                                <img src="@Model.Attach" alt="image" width="300" height="240" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section VendorScripts{
    <script src="../../../assets/vendor/jquery-steps/jquery.steps.js"></script> <!-- JQuery Steps Plugin Js -->
    <script src="../../../assets/vendor/dropify/js/dropify.min.js"></script>
    <script src="../../../assets/bundles/mainscripts.bundle.js"></script>

    <script type="text/javascript">
        $(document).ready(function () {
            $('.custom-select').attr("disabled", "disabled");
        });
    </script>
}

@section Scripts{
    <script src="../../../assets/js/pages/forms/form-wizard.js"></script>
    <script src="../../../assets/js/pages/forms/dropify.js"></script>
}