@using AwesomeCare.Admin.ViewModels.Staff;
@model CreateStaffKeyWorkerVoice
@{
    ViewData["Title"] = "View Staff Key Voice Worker";
}
@section HeaderScripts{
    <link rel="stylesheet" href="../../../assets/vendor/jquery-steps/jquery.steps.css">
    <link rel="stylesheet" href="../../../assets/vendor/dropify/css/dropify.min.css">
}
<div class="row clearfix">
    <div class="col-lg-12 col-md-12">
        <div class="card planned_task">
            <div class="header">

                <h2></h2>

            </div>
            <div class="body">
                <div class="row">
                    <div class="col-xs-8 col-sm-8 col-md-8">
                        <input type="hidden" asp-for="KeyWorkerId" />
                        <input type="hidden" asp-for="StaffId" />
                        <div class="row">
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">DATE</label>
                                <input type="text" asp-for="Date" class="form-control" readonly />
                            </div>
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">NEXT DUE DATE</label>
                                <input type="text" asp-for="NextCheckDate" class="form-control" readonly />
                            </div>
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">DEADLINE</label>
                                <input type="text" asp-for="Deadline" class="form-control" readonly />
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">Team You Work For</label>
                                <select asp-for="TeamYouWorkFor" asp-items="@Model.WorkList" class="selectpicker" data-live-search="true" multiple></select>
                            </div>
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">Uncomfortable Users Services</label>
                                <select class="custom-select" asp-for="NotComfortableServices" asp-items="@Model.ClientList">
                                </select>
                            </div>
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">Requiring More Time</label>
                                <select class="custom-select" asp-for="ServicesRequiresTime" asp-items="@Model.ClientList">
                                </select>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">Requiring More Services</label>
                                <select class="custom-select" asp-for="ServicesRequiresServices" asp-items="@Model.ClientList">
                                </select>
                            </div>
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">Details</label>
                                <baserecord asp-key="Details" asp-for="Details" asp-items="null" />
                            </div>
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">Officer to Take Action</label>
                                <select asp-for="OfficerToAct" asp-items="@Model.OfficerToActList" class="selectpicker" data-live-search="true" multiple></select>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">Well Supported Services</label>
                                <baserecord asp-key="Well_Supported_Services" asp-for="WellSupportedServices" asp-items="null" />
                            </div>
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">Status</label>
                                <baserecord asp-key="Staff_Communication_Status" asp-for="Status" asp-items="null" />
                            </div>
                            <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                <label class="control-label">Reference</label>
                                <input type="text" asp-for="Reference" class="form-control" readonly />
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                <label class="control-label">URL</label>
                                <input type="text" asp-for="URL" class="form-control" readonly />
                            </div>
                            <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                <label class="control-label">Changes We Need To Make Client Happy</label>
                                <textarea type="text" asp-for="ChangesWeNeed" class="form-control" readonly></textarea>
                            </div>
                            <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                <label class="control-label">Nutritional Changes</label>
                                <textarea type="text" asp-for="NutritionalChanges" class="form-control" readonly></textarea>
                            </div>
                            <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                <label class="control-label">Health And WellNess Changes</label>
                                <textarea type="text" asp-for="HealthAndWellNessChanges" class="form-control" readonly></textarea>
                            </div>
                            <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                <label class="control-label">Medication Changes</label>
                                <textarea type="text" asp-for="MedicationChanges" class="form-control" readonly></textarea>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                <label class="control-label">Moving And Handling</label>
                                <textarea type="text" asp-for="MovingAndHandling" class="form-control" readonly></textarea>
                            </div>
                            <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                <label class="control-label">Risk Assessment</label>
                                <textarea type="text" asp-for="RiskAssessment" class="form-control" readonly></textarea>
                            </div>
                            <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                <label class="control-label">Remrks</label>
                                <textarea type="text" asp-for="Remarks" class="form-control" readonly></textarea>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                <label class="control-label">Action Required</label>
                                <textarea type="text" asp-for="ActionRequired" class="form-control" readonly></textarea>
                            </div>

                        </div>
                    </div>
                    <div class="col-xs-4 col-sm-4 col-md-4">
                        <div class="card">
                            <div class="header">
                                <h2>Attachment Image<small></small></h2>
                            </div>
                            <div class="body">
                                <img src="@Model.Attach" alt="image" width="300" height="240" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section VendorScripts{
    <script src="../../../assets/vendor/jquery-steps/jquery.steps.js"></script> <!-- JQuery Steps Plugin Js -->
    <script src="../../../assets/vendor/dropify/js/dropify.min.js"></script>
    <script src="../../../assets/bundles/mainscripts.bundle.js"></script>

    <script type="text/javascript">
        $(document).ready(function () {
            $('.custom-select').attr("disabled", "disabled");
        });
    </script>
}

@section Scripts{
    <script src="../../../assets/js/pages/forms/form-wizard.js"></script>
    <script src="../../../assets/js/pages/forms/dropify.js"></script>
}

