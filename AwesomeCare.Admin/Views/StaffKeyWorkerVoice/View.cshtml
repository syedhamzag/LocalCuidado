@using AwesomeCare.Admin.ViewModels.Staff;
@model CreateStaffKeyWorkerVoice
@{
    ViewData["Title"] = "View Staff Key Voice Worker";
}
@section HeaderScripts{
    <link rel="stylesheet" href="../../../assets/vendor/jquery-steps/jquery.steps.css">
    <link rel="stylesheet" href="../../../assets/vendor/dropify/css/dropify.min.css">
}
<div class="row clearfix">
    <div class="col-lg-12 col-md-12">
        <div class="card planned_task">
            <div class="header">

                <h2></h2>

            </div>
            <div class="body">
                <form asp-route="Edit" method="post" enctype="multipart/form-data">
                    <div class="row">
                        <div class="col-xs-8 col-sm-8 col-md-8">
                            <input type="hidden" asp-for="KeyWorkerId" />
                            <input type="hidden" asp-for="StaffId" />
                            <div class="row">
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">DATE</label>
                                    <label class="control-label">@Model.Date</label>
                                </div>
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">NEXT DUE DATE</label>
                                    <label class="control-label">@Model.NextCheckDate</label>
                                </div>
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">DEADLINE</label>
                                    <label class="control-label">@Model.Deadline</label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">Team You Work For</label>
                                    <label class="control-label">@Model.WorkteamName.ToList()</label>
                                </div>
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">Uncomfortable Users Services</label>
                                    <label class="control-label">@Model.NotComfortableServices</label>
                                </div>
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">Requiring More Time</label>
                                    <label class="control-label">@Model.ServicesRequiresTime</label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">Requiring More Services</label>
                                    <label class="control-label">@Model.ServicesRequiresServices</label>
                                </div>
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">Details</label>
                                    <label class="control-label">@Model.Details</label>
                                </div>
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">Officer to Take Action</label>
                                    <label class="control-label">@Model.OfficerName.ToList()</label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">Well Supported Services</label>
                                    <label class="control-label">@Model.WellSupportedServices</label>
                                </div>
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">Status</label>
                                    <label class="control-label">@Model.Status</label>
                                </div>
                                <div class="form-group col-sm-4 col-xs-12 col-md-4 col-lg-4">
                                    <label class="control-label">Reference</label>
                                    <label class="control-label">@Model.Reference</label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                    <label class="control-label">URL</label>
                                    <label class="control-label">@Model.URL</label>
                                </div>
                                <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                    <label class="control-label">Changes We Need To Make Client Happy</label>
                                    <label class="control-label">@Model.ChangesWeNeed</label>
                                </div>
                                <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                    <label class="control-label">Nutritional Changes</label>
                                    <label class="control-label">@Model.NutritionalChanges</label>
                                </div>
                                <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                    <label class="control-label">Health And WellNess Changes</label>
                                    <label class="control-label">@Model.HealthAndWellNessChanges</label>
                                </div>
                                <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                    <label class="control-label">Medication Changes</label>
                                    <label class="control-label">@Model.MedicationChanges</label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                    <label class="control-label">Moving And Handling</label>
                                    <label class="control-label">@Model.MovingAndHandling</label>
                                </div>
                                <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                    <label class="control-label">Risk Assessment</label>
                                    <label class="control-label">@Model.RiskAssessment</label>
                                </div>
                                <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                    <label class="control-label">Remrks</label>
                                    <label class="control-label">@Model.Remarks</label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-sm-12 col-xs-12 col-md-12 col-lg-12">
                                    <label class="control-label">Action Required</label>
                                    <label class="control-label">@Model.ActionRequired</label>
                                </div>

                            </div>
                        </div>
                        <div class="col-xs-4 col-sm-4 col-md-4">
                            <div class="card">
                                <div class="header">
                                    <h2>Attachment Image<small></small></h2>
                                </div>
                                <div class="body">
                                    <img src="@Model.Attachment" alt="image" />
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-sm-6 col-xs-12 col-md-4 col-lg-6">
                                    <button type="submit" class="btn btn-primary pull-right">Submit</button>
                                </div>
                                <div class="form-group col-sm-6 col-xs-12 col-md-4 col-lg-6">
                                    <a asp-action="Index" asp-route-staffId="@Model.StaffId" class="btn btn-primary pull-left">Cancel</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
@section VendorScripts{
    <script src="../../../assets/vendor/jquery-steps/jquery.steps.js"></script> <!-- JQuery Steps Plugin Js -->
    <script src="../../../assets/vendor/dropify/js/dropify.min.js"></script>
    <script src="../../../assets/bundles/mainscripts.bundle.js"></script>
}

@section Scripts{
    <script src="../../../assets/js/pages/forms/form-wizard.js"></script>
    <script src="../../../assets/js/pages/forms/dropify.js"></script>
}

